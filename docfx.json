{
  "metadata": [
    {
      "src": [
        {
          "src": ".",
          "files": [
            "TrinketTinker/TrinketTinker.csproj"
          ]
        }
      ],
      "dest": "api",
      "filter": "docfx-api-filters.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      },
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "TrinketTinker",
      "_appTitle": "TrinketTinker",
      "_appLogoPath": "images/favicon.png",
      "_appFaviconPath": "images/favicon.png",
      "_enableSearch": true,
      "_gitUrlPattern": "github",
      "pdf": false
    }
  }
}