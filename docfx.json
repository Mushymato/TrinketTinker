{
  "metadata": [
    {
      "src": [
        {
          "src": ".",
          "files": [
            "TrinketTinker/TrinketTinker.csproj"
          ]
        }
      ],
      "dest": "api",
      "filter": "docfx-api-filters.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "guide/tinker/*.{md,yml}"
        ]
      },
      {
        "files": [
          "guide/trinket/*.{md,yml}"
        ]
      },
      {
        "files": [
          "api/*"
        ]
      },
      {
        "files": [
          "toc.yml",
          "index.md",
        ]
      },
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "guide/**/*.{json}"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "TrinketTinker",
      "_appTitle": "TrinketTinker",
      "_appLogoPath": "images/favicon.png",
      "_appFaviconPath": "images/favicon.png",
      "_enableSearch": false,
      "pdf": false
    }
  }
}