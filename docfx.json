{
  "metadata": [
    {
      "src": [
        {
          "src": ".",
          "files": [
            "TrinketTinker/TrinketTinker.csproj"
          ]
        }
      ],
      "dest": "api",
      // "filter": "docfx-api-filters.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      },
      "globalMetadata": {
        "_appTitle": "My App"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "TrinketTinker",
      "_appTitle": "TrinketTinker",
      "_enableSearch": true,
      "pdf": false
    }
  }
}